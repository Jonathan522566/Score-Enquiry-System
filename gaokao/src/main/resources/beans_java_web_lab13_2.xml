<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 分别通过byName、byType和constructor三种方式自动装配 -->
    <bean id="cat" class="com.example.java_web_lab13_2.Cat">
        <property name="name" value="mimi"/>
        <property name="age" value="3"/>
    </bean>
    <bean id="dog" class="com.example.java_web_lab13_2.Dog">
        <property name="name" value="wangwang"/>
        <property name="age" value="4"/>
    </bean>
    <!--    <bean id="person" class="com.sise.bean.Person" autowire="byName">-->
    <!--        <property name="PName" value="jhy"/>-->
    <!--    </bean>-->
    <bean id="person1" class="com.example.java_web_lab13_2.Person" autowire="byType">
        <property name="pName" value="jhy"/>
    </bean>
</beans>